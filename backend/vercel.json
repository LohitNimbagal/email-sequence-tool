{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/dist/(.*)",
            "dest": "/",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": [
                {
                    "source": "https://emailsequence.vercel.app/*",
                    "headers": [
                        {
                            "key": "Access-Control-Allow-Origin",
                            "value": "*"
                        }
                    ]
                }
            ]
        }
    ]
}